{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue"],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "types": [
      "element-plus/global", //element-plus自动引入
      "@vue-macros/reactivity-transform/macros-global" //类型ReactivityTransform响应式数据$ref省略.value
    ],

    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
