// 模型商品表
model v2_cart {
    card_id    String @id @default(cuid()) //购车id
    user_id    String @default("") // 用户id
    author_id  String @default("") // 作者id
    product_id String @default("") // 商品id

    price_type String @default("price_free") // 价格类型 //price_free 免费 price_model 模型价格 price_print 打印价格
    count      Int    @default(1) // 商品的数量

    is_deleted Boolean  @default(false) // 是否删除
    updated_at DateTime @updatedAt
    created_at DateTime @default(now())
}
