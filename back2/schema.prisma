generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./sqlite.db"
}

model tb_user {
  id         Int      @id @default(autoincrement())
  phone      String   @unique
  password   String
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt
}
